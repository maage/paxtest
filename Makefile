all:
	@cat targets

adamantix:
	make -f Makefile.Adamantix

gentoo:
	make -f Makefile.Gentoo

generic:
	make -f Makefile.generic

openbsd:
	gmake -f Makefile.OpenBSD

clean:
	make -f Makefile.Adamantix clean

